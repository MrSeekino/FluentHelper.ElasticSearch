<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Description>An helper for projects using ElasticSearch 7.x wrapping NEST package</Description>
		<Nullable>enable</Nullable>
		<PackageId>FluentHelper.ElasticSearch</PackageId>
		<Title>FluentHelper.ElasticSearch</Title>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Version>7.0.0</Version>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<Product>FluentHelper.ElasticSearch</Product>
		<PackageProjectUrl>https://github.com/MrSeekino/FluentHelper.ElasticSearch</PackageProjectUrl>
		<RepositoryUrl>https://github.com/MrSeekino/FluentHelper.ElasticSearch</RepositoryUrl>
		<PackageTags>FluentHelper;ElasticSearch;NEST</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Logging" Version="6.0.0" />
		<PackageReference Include="NEST" Version="7.17.5" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	</ItemGroup>

</Project>
